{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:ssr": "vite build && vite build --ssr",
        "dev": "vite",
        "server": "node server.cjs",
        "format": "prettier --write resources/",
        "format:check": "prettier --check resources/",
        "lint": "eslint . --fix",
        "types": "tsc --noEmit",
        "route": "php artisan route:cache && php artisan route:clear && php artisan route:list",
        "migrate": "php artisan migrate:fresh --seed",
        "optimize": "php artisan optimize:clear && php artisan optimize",
        "serve": "concurrently \"php artisan serve\" \"bun run vite\" \"php artisan reverb:start\"",
        "pin": "php artisan make:command GenerateEmployeePins",
        "push-kyle": "bun run scripts/push.js",
        "push-main": "git merge kyle && git push -u origin main"
    },
